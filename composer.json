{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "bissolli/nova-phone-field": "^0.0.4",
        "ebess/advanced-nova-media-library": "^3.6",
        "eibrahimli/column-permissions-field": "*",
        "eibrahimli/contact-phones-fields": "*",
        "eibrahimli/hidden-field": "*",
        "eminiarts/nova-tabs": "^1.4",
        "epartment/nova-dependency-container": "^1.3",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "koss-shtukert/laravel-nova-select2-auto-complete": "^1.21",
        "laravel/framework": "^8.54",
        "laravel/nova": "*",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.5",
        "optimistdigital/nova-tailwind": "^1.1",
        "orlyapps/nova-belongsto-depend": "^3.0",
        "saintsystems/nova-resource-group-menu": "^0.1.0",
        "spatie/laravel-medialibrary": "^9.8",
        "yassi/nova-nested-form": "^3.0",
        "zareismail/nova-wizard": "^0.1.0",
        "eibrahimli/customer-loan-field": "*"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.8"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        },
        {
            "type": "path",
            "url": "./nova-components/ContactPhonesFields"
        },
        {
            "type": "path",
            "url": "./nova-components/HiddenField"
        },
        {
            "type": "path",
            "url": "./nova-components/ColumnPermissionsField"
        },
        {
            "type": "path",
            "url": "./nova-components/CustomerLoanField"
        }
    ]
}